[project]
	description = KNoT control service for the gateway. It is responsible for start/stop/configure all other services running in the gateway.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Rodrigo Perazzo (rodrigoperazzo)
[access "refs/for/refs/*"]
	read = group user/Rodrigo Perazzo (rodrigoperazzo)
	push = group user/Rodrigo Perazzo (rodrigoperazzo)
	push = group user/Thiago Figueredo Cardoso (figueredo)
	label-Code-Review = -2..+2 group user/Rodrigo Perazzo (rodrigoperazzo)
	label-Code-Review = -2..+2 group user/Thiago Figueredo Cardoso (figueredo)
	label-Verified = -1..+1 group user/Rodrigo Perazzo (rodrigoperazzo)
	label-Verified = -1..+1 group user/Thiago Figueredo Cardoso (figueredo)
	rebase = group user/Rodrigo Perazzo (rodrigoperazzo)
	removeReviewer = group user/Rodrigo Perazzo (rodrigoperazzo)
	submit = group user/Rodrigo Perazzo (rodrigoperazzo)
	submit = group user/Thiago Figueredo Cardoso (figueredo)
[access "refs/heads/*"]
	read = group user/Rodrigo Perazzo (rodrigoperazzo)
	create = group user/Rodrigo Perazzo (rodrigoperazzo)
	push = +force group user/Rodrigo Perazzo (rodrigoperazzo)
	pushMerge = group user/Rodrigo Perazzo (rodrigoperazzo)
[access "refs/tags/*"]
	push = group user/Rodrigo Perazzo (rodrigoperazzo)
	push = +force group user/Rodrigo Perazzo (rodrigoperazzo)
[submit]
	action = rebase always
